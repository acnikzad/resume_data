@students.map do |student|
  {
    id: student.id,
    first_name: student.first_name,
    last_name: student.last_name,
    email: student.email,
    phone: student.phone,
    bio: student.bio,
    linkedin: student.linkedin,
    twitter: student.twitter,
    website: student.website,
    resume: student.resume,
    github: student.github,
    photo: student.photo
  }
end
